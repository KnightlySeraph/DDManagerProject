<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <Color x:Key="PanelSeperationColor">LightGray</Color>
    <SolidColorBrush x:Key="PanelBorderBrush" Color="{StaticResource PanelSeperationColor}"/>

    <Color x:Key="MouseOverColor">LightBlue</Color>
    <SolidColorBrush x:Key="MouseOverBrush" Color="{StaticResource MouseOverColor}"/>

</ResourceDictionary>